<div>
  <h1 class="font-bold text-4xl">Links#new</h1>
  <%= form_with model: link do |form| %>
    <%= form.label :display %>
    <%= form.check_box :display %>
    <%= form.label :icon %>
    <%= form.text_field :icon %>
    <%= form.label :url %>
    <%= form.url_field :url %>
    <%= form.submit %>
  <% end %>
</div>
